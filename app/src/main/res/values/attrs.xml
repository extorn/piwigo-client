<resources>

    <!-- my custom style attribs -->
    <attr format="reference" name="materialButtonIconStyle"/>
    <attr format="reference" name="materialButtonIconAccentedStyle"/>
    <attr format="reference" name="checkboxFavoriteAccentedStyle"/>
    <attr format="reference" name="visibilityToggleStyle"/>
    <attr format="reference" name="editableFieldCheckBox"/>
    <attr format="reference" name="clickableTextViewStyleBody1"/>
    <attr format="reference" name="clickableTextViewStyleBody1Field"/>
    <attr format="reference" name="clickableTextViewStyleBody2"/>
    <attr format="reference" name="clickableTextViewStyleTitle"/>
    <attr format="reference" name="tabLayout"/>
    <attr format="reference" name="editableFieldLabel"/>
    <attr format="reference" name="editableFieldText"/>
    <attr format="reference" name="editableFieldSpinner"/>
    <attr format="reference" name="editableFieldSwitch"/>
    <attr format="reference" name="editableFieldTextClickable"/>
    <attr format="reference|color" name="scrimUltraHeavy"/>
    <attr format="reference|color" name="scrimHeavy"/>
    <attr format="reference|color" name="scrimLight"/>

    <attr format="color" name="colorPrimaryDisabled"/>
    <attr format="color" name="colorAlbumItemBackground"/>



    <!-- Declare custom theme attributes that allow changing which styles are
         used for button bars depending on the API privacyLevel.
         ?android:attr/buttonBarStyle is pkg as of API 11 so this is
         necessary to support previous API levels. -->



    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="FlowLayout">
        <attr name="horizontalSpacing" format="dimension"/>
        <attr name="verticalSpacing" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="FlowLayout_LayoutParams">
        <attr name="layout_breakLine" format="boolean" />
        <attr name="layout_horizontalSpacing" format="dimension" />
    </declare-styleable>

    <attr name="initialTabIdx" format="integer" />

    <declare-styleable name="CustomMaterialTabLayout">
        <attr name="initialTabIdx"/>
    </declare-styleable>

    <declare-styleable name="SlidingTabLayout">
        <attr name="viewPager" format="reference" />
        <attr name="initialTabIdx"/>
        <attr name="tabTitleViewLayoutId" format="reference"/>
        <attr name="tabTitleTextViewId" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CustomExoPlayerTimeBar">
        <attr name="locally_cached_color" format="color"/>
        <attr name="unplayed_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="CustomEditTextPreference">
        <attr name="android:inputType"/>
    </declare-styleable>

    <declare-styleable name="SecureEditTextPreference">
    </declare-styleable>

    <declare-styleable name="NumberPickerPreference">
        <attr name="maxValue" format="integer"/>
        <attr name="minValue" format="integer"/>
        <attr name="multiplier" format="integer"/>
        <attr name="wrapPickList" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ClientCertificatePreference">
    </declare-styleable>

    <declare-styleable name="MaterialCheckboxTriState">
        <attr name="state_always_checked" format="boolean"/>
        <attr name="checkbox_at_end" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="EditableListPreference">
        <attr name="entriesPref" format="reference"/>
        <attr name="allowItemEdit" format="boolean"/>
        <attr name="allowMultiSelect" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MappedListPreference">
        <attr name="entries" format="reference"/>
        <attr name="entryValues" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ServerAlbumListPreference">
        <attr name="connectionProfileNameKey"/>
    </declare-styleable>

    <declare-styleable name="ServerAlbumSelectPreference">
        <attr name="connectionProfileNameKey"/>
    </declare-styleable>

    <attr name="connectionProfileNameKey" format="string"/>

    <declare-styleable name="SlidingBottomSheet">
        <attr name="appBarViewId" format="reference"/>
        <attr name="stateExpanded" format="boolean"/>
    </declare-styleable>

</resources>
