<resources>

    <!-- my custom style attribs -->
    <attr format="reference" name="materialButtonIconStyle"/>
    <attr format="reference" name="materialButtonIconAccentedStyle"/>
    <attr format="reference" name="checkboxFavoriteAccentedStyle"/>
    <attr format="reference" name="visibilityToggleStyle"/>
    <attr format="reference" name="layoutExpansionToggleStyle"/>
    <attr format="reference" name="editableFieldCheckBox"/>
    <attr format="reference" name="clickableTextViewStyleBody1"/>
    <attr format="reference" name="clickableTextViewStyleBody2"/>
    <attr format="reference" name="clickableTextViewStyleTitle"/>
    <attr format="reference" name="appTabLayout"/>
    <attr format="reference" name="editableFieldLabel"/>
    <attr format="reference" name="editableFieldText"/>
    <attr format="reference" name="editableFieldSpinner"/>
    <attr format="reference" name="editableFieldSwitch"/>
    <attr format="reference" name="editableFieldTextClickable"/>
    <attr format="reference" name="scrimUltraHeavy"/>
    <attr format="reference" name="scrimHeavy"/>
    <attr format="reference" name="scrimLight"/>
    <attr format="reference" name="appNumberPickerStyle"/>
    <attr format="reference" name="editableFieldNumberPicker"/>
    <attr format="color" name="appScrimColor"/>
    <attr format="color" name="colorAlbumItemBackground"/>



    <!-- Declare custom theme attributes that allow changing which styles are
         used for button bars depending on the API privacyLevel.
         ?android:attr/buttonBarStyle is pkg as of API 11 so this is
         necessary to support previous API levels. -->




    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="CustomExoPlayerTimeBar">
        <attr name="locally_cached_color" format="color"/>
        <attr name="unplayed_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="ClientCertificatePreference">
    </declare-styleable>


    <declare-styleable name="EditableListPreference">
        <attr name="entriesPref" format="reference"/>
        <attr name="allowItemEdit" format="boolean"/>
        <attr name="allowMultiSelect" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SecureEditTextPreference">
    </declare-styleable>

    <declare-styleable name="ServerAlbumListPreference">
        <attr name="connectionProfileNameKey"/>
    </declare-styleable>

    <declare-styleable name="ServerAlbumSelectPreference">
        <attr name="connectionProfileNameKey"/>
    </declare-styleable>

    <attr name="connectionProfileNameKey" format="string"/>



</resources>
